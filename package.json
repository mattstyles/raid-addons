{
  "name": "raid-addons",
  "version": "0.0.0",
  "description": "Add-ons functions for use with Raid",
  "main": "lib/",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "browserify": {
    "transform": "babelify"
  },
  "babel": {
    "presets": [
      "urban"
    ]
  },
  "keywords": [
    "raid",
    "addons",
    "helpers",
    "utility"
  ],
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/raid-addons"
  },
  "bugs": "https://github.com/mattstyles/raid-addons/issues",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-preset-urban": "^0.4.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.0.4",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "quay": "^0.4.1",
    "raf-stream": "^3.0.0",
    "raid": "^0.5.0",
    "ramda": "^0.21.0",
    "rx-lite": "^4.0.8"
  }
}
